/* pages/mine/mine.wxss */
page{
  background: #f0f3fa;
}

.top-black{
  width:100%;
  height:45rpx;
  background: black;
}
.container{
  background: #f0f3fa;
}
.top{
  width:100%;
  height:410rpx;
    padding-top:40rpx;
  background: url('data:image/jpeg;base64,') no-repeat;
  background-size:750rpx 450rpx; 
}

/* 第一行关注、头像、粉丝 */
.top-line1{
  display: flex;
  justify-content:center;
  margin-top:80rpx;
}

.top-line1-left,.top-line1-right{
  background-color:rgba(0,0,0,0.5);
  color:white;
  font-size:22rpx;
  width:90rpx;
  height:90rpx;
  border-radius: 90rpx;
  text-align: center;
  margin-top:15rpx;
}

.top-line1-text{
  width:90rpx;
  height:45rpx;
}

.top-line1-right{
  margin-left:50rpx;
}

.pic-outcontainer{
  width: 122rpx;
  height: 122rpx;
  border-radius: 122rpx;
  background: white;
  margin-left:50rpx;
}

.top-line1-mid{
  width:120rpx;
  height:120rpx;
  border: 1rpx solid white;
  border-radius: 120rpx;
}

.top-line1-mid-icon{
  position: relative;
  width:40rpx;
  height:40rpx;
  border-radius: 40rpx;
  top:-44rpx;
  left:89rpx;
}

.top-line1-nickname{
  clear: both;
  display: flex;
  justify-content: center;
  width:400rpx;
  margin:0rpx auto;
  height:80rpx;
  line-height: 80rpx;
}

.top-line1-nickname-text{
  font-size:25rpx;
  color:white;
  height:80rpx;
  line-height: 80rpx;
  letter-spacing: 1rpx;
}

.top-line1-nickname-icon{
  width:30rpx;
  height:30rpx;
  margin-top:25rpx;
  margin-left:15rpx;
}

/* 头部第二行积分、学币、学时 */
.top-line2{
  display: flex;
  justify-content: space-around;
  padding-left:50rpx;
  padding-right:50rpx;
  margin-top:20rpx;
}

.top-line2-block{
  color:white;
  font-size:23rpx;
  width:215rpx;
  height:80rpx;
}

.top-line2-text{
  width:215rpx;
  height:40rpx;
  line-height: 40rpx;
  text-align: center;
}

.line{
  border-left:1rpx solid  white;
  height:80rpx;
}

/* 中间部分 */
.mid{
  background: white;
  height:374rpx;
}
.mid-line-item{
  display: float;
  float:left;
  width:187rpx;
  height:187rpx;
}

.mid-line-item-icon{
  width:60rpx;
  height:60rpx;
  margin-left:63rpx;
  margin-top:45rpx;
}

.mid-line-item-text{
  font-size:22rpx;
  width:187;
  text-align: center;
  letter-spacing: 1rpx;
}

.message{
  position: relative;
  width:30rpx;
  height:30rpx;
  border-radius: 30rpx;
  line-height: 30rpx;
  background: rgb(255, 60, 0);
  text-align: center;
  color:white;
  font-size:20rpx;
  left:105rpx;
  top:-113rpx;
  z-index: 10001
}

/* 退出登录 */
.loginout{
  background: linear-gradient(to right,rgb(253, 172, 96),rgb(255, 123, 0));
  width:90%;
  height:70rpx;
  line-height: 70rpx;
  color:white;
  text-align: center;
  margin:100rpx auto;
  font-size:25rpx;
  border-radius: 10rpx;
}

.select1{
  background: rgb(236, 236, 236);
}

.select2{
  background: rgb(255, 94, 0);
}

.select3{
  background: rgb(211, 211, 211);
}

.select4{
  background: rgba(255, 255, 255,0.1);
}